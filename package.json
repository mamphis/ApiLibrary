{
  "name": "@mamphis/api",
  "workspaces": [
    "api-node",
    "api-vue"
  ],
  "scripts": {
    "dep": "npm ci --workspaces",
    "build": "npm run build --workspaces",
    "updateVersion": "npm version patch --workspaces --git-tag-version=false",
    "publish": "npm run build:all && npm run updateVersion && npm run publish --workspaces"
  }
}
